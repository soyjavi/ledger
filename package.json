{
  "name": "ledger",
  "description": "Your private finance ledger running on your own blockchain",
  "version": "0.27.18",
  "license": "LGPL-3.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "setup": "rm -rf node_modules && rm -rf yarn.lock && yarn install --force",
    "start": "expo start --web",
    "start:prod": "expo start --no-dev --minify --web",
    "build:android": "NODE_ENV=production expo build:android",
    "build:ios": "NODE_ENV=production expo build:ios",
    "build:ota": "NODE_ENV=production expo publish",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint --cache --fix --format codeframe --ext .jsx,.js,.tsx,.ts src",
    "preversion": "yarn fixer",
    "version": "git add -A dist",
    "postversion": "git push && git push --tags",
    "release-it": "yarn version --patch && git push --follow-tags"
  },
  "dependencies": {
    "@lookiero/aurora-next": "^3.0",
    "@lookiero/event-next": "^0.3",
    "@lookiero/router": "^0.3",
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/netinfo": "5.9.7",
    "color": "^3.1.2",
    "expo-barcode-scanner": "~9.1.0",
    "expo-blur": "~8.2.2",
    "expo-camera": "~9.1.0",
    "expo-crypto": "~8.4.0",
    "expo-local-authentication": "^11.1.1",
    "expo-location": "~10.0.0",
    "expo-permissions": "~10.0.0",
    "expo-status-bar": "~1.0.3",
    "global": "^4.4.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-fs": "^2.16.6",
    "react-native-web": "~0.13.12",
    "vanilla-blockchain": "^0.2.9",
    "vanilla-storage": "^0.3.13"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@lookiero/eslint": "^0.1",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "^3.15.2",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "expo": "^41.0.1",
    "jest-expo": "^41.0.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "verbose": false
  }
}
