{
  "name": "voltvault-app",
  "description": "Your private finance ledger running on your own blockchain",
  "version": "0.22.10",
  "license": "LGPL-3.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build:apk": "NODE_ENV=production expo build:android",
    "build:ota": "NODE_ENV=production expo publish",
    "start": "expo start --web",
    "start:prod": "expo start --no-dev --minify --web",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint '**/*.{js,jsx}' --fix ",
    "preversion": "yarn fixer",
    "version": "git add -A dist",
    "postversion": "git push && git push --tags",
    "release-it": "yarn version --patch && git push --follow-tags"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.2.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/netinfo": "5.9.2",
    "expo": "~38.0.1",
    "expo-barcode-scanner": "^8.2.1",
    "expo-camera": "^8.3.1",
    "expo-crypto": "~8.2.1",
    "expo-font": "^8.2.1",
    "expo-local-authentication": "^9.1.1",
    "expo-location": "^8.2.1",
    "expo-permissions": "^9.0.1",
    "expo-status-bar": "^1.0.0",
    "global": "^4.4.0",
    "prop-types": "^15.7.2",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-fs": "^2.16.6",
    "react-native-web": "~0.11.7",
    "vanilla-blockchain": "^0.2.8",
    "vanilla-storage": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-react-native": "^3.8.1",
    "jest-expo": "^38.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/reactor/"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "verbose": false
  }
}
