{
  "name": "voltvault-app",
  "description": "Cross-Device client for voltvault",
  "version": "0.18.45",
  "license": "LGPL-3.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build:apk": "NODE_ENV=production expo build:android",
    "build:ota": "NODE_ENV=production expo publish",
    "start": "expo start --web",
    "start:prod": "expo start --no-dev --minify --web",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "fixer": "yarn prettier && yarn linter",
    "prettier": "prettier '**/*.{js,ts,tsx}' --write",
    "linter": "eslint '**/*.{js,jsx}' --fix "
  },
  "dependencies": {
    "@expo/vector-icons": "~10.0.6",
    "@react-native-community/netinfo": "4.6.0",
    "expo": "~36.0.0",
    "expo-barcode-scanner": "~8.0.0",
    "expo-camera": "~8.0.0",
    "expo-font": "~8.0.0",
    "expo-local-authentication": "~8.0.0",
    "expo-location": "~8.0.0",
    "expo-permissions": "~8.0.0",
    "global": "~4.4.0",
    "prop-types": "~15.7.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-eslint": "~10.0.3",
    "babel-preset-expo": "~8.0.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "~7.17.0",
    "eslint-plugin-react-hooks": "~2.3.0",
    "eslint-plugin-react-native": "~3.8.1",
    "jest-expo": "~36.0.1",
    "prettier": "~1.19.1",
    "react-test-renderer": "^16.12.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "node_modules/",
      "src/reactor/"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "verbose": false
  }
}
